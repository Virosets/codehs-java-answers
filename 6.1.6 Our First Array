public class FirstArray
{
    public static void main(String[] args)
    {
      // Create the 3 arrays here
      String[] cities = {"Las Vegas", "Minsk", "Sao Paulo"};
      
      int[] temp = {104, 73, 80};
      
      double[] rain = {4.17, 26.7, 55.0};
        
      // Print all 3 arrays according to the output in the description
      System.out.println(cities[0] + " has an average annual precipitation of " + rain[0] + " inches.");
      System.out.println(cities[0] + " has an average annual high temp of " + temp[0] + " degrees Fahrenheit.");
      System.out.println(cities[1] + " has an average annual precipitation of " + rain[1] + " inches.");
      System.out.println(cities[1] + " has an average annual high temp of " + temp[1] + " degrees Fahrenheit.");
      System.out.println(cities[2] + " has an average annual precipitation of " + rain[2] + " inches.");
      System.out.println(cities[2] + " has an average annual high temp of " + temp[2] + " degrees Fahrenheit.");
        
        
        
        
    }
}
