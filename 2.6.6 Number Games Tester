public class GamesTester
{
    public static void main(String[] args)
    {
        NumberGames game = new NumberGames(5);
        
        // Square the number
        // Print it out
        double squareNumber = game.squareNumber();
        System.out.println(squareNumber);
        
        // Double the number
        // Print it out
        double doubleIt = game.doubleNumber();
        System.out.println(doubleIt);
        
        // Square the number again
        // Print it out
        double number = game.squareNumber();
        System.out.println(number);
        
        // Get the number and store the value
        // Print out your stored value 
        double storeNum = game.getNumber();
        System.out.println(storeNum);
        
        // Get the number again then store and print
        // the value
        double storeNumAgain = game.getNumber();
        System.out.println(storeNumAgain);
        
    }
}
