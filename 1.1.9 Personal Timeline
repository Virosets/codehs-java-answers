public class Timeline
{
    public static void main(String[] args)
    {
        // Start here!
        System.out.println("Age | m/yy - description");
        System.out.println("4   | 4/11 - First Vaction");
        System.out.println("    |");
        System.out.println("    |");
        System.out.println("    |");

        System.out.println("6   | 4/13 - Saw my first hockey game");
        System.out.println("    | 6/13 - Saw a bat");
        System.out.println("    | 11/13 - I like trains");
        System.out.println("    |");
        
        System.out.println("8   | 5/15 - Got into reading books");
        System.out.println("    |");
        System.out.println("    |");
        System.out.println("    |");
        
        System.out.println("10  | 9/17 - Got my own library card");
        System.out.println("    |");
        System.out.println("    |");
        System.out.println("    |");
        
        System.out.println("12  | 10/19 - Taught brother how to skate");
        System.out.println("    |");
        System.out.println("    |");
        System.out.println("    |");
        
        System.out.println("14  |");
        System.out.println("    |");
        System.out.println("    |");
        System.out.println("    |");
        
        System.out.println("16  |");
    }
}
