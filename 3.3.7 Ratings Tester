public class RaterTesting
{
    public static void main(String[] args)
    {
        // Start here!
        Rater bread = new Rater("BreadMan", 2);
        
        bread.updateReview();
        System.out.println(bread);
        bread.setRating(4);
        bread.updateReview();
        System.out.println(bread);
    }
}
