public class Chess
{
    public static void main(String[] args)
    {
        //Create an 8x8 2D String array called chess.
        String[] chess1 = {"Castle","Knight","Bishop","Queen","King","Bishop","Knight","Castle"};
        String[] chess2 = {"Pawn","Pawn","Pawn","Pawn","Pawn","Pawn","Pawn","Pawn"};
        String[] chess3 = {"-","-","-","-","-","-","-","-"};
        String[] chess4 = {"-","-","-","-","-","-","-","-"};
        String[] chess5 = {"-","-","-","-","-","-","-","-"};
        String[] chess6 = {"-","-","-","-","-","-","-","-"};
        String[] chess7 = {"Pawn","Pawn","Pawn","Pawn","Pawn","Pawn","Pawn","Pawn"};
        String[] chess8 = {"Castle","Knight","Bishop","Queen","King","Bishop","Knight","Castle"};
        
        String[][] chess = {chess1, chess2, chess3, chess4, chess5, chess6, chess7, chess8};
        //Use this method to print the chess board onto the console
        printBoard(chess);
        

    }
    
    //Do not make alterations to this method!
    public static void printBoard(String[][] array)
    {
        for(String[] row: array)
        {
            for(String thing: row)
            {
                System.out.print(thing + "\t");
            }
            System.out.println();
        }
    }
}
