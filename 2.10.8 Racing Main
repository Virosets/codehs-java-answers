public class RaceMain
{
    public static void main(String[] args)
    {
        // Length of the course in meters
        double distance = 2414; // ~ 1.5 miles
        
        // Generate a random acceleration for each car
        double ranNum = (Math.random() * (100 + 1) + 1);
        double ranNum2 = (Math.random() * (100 + 1) + 1);
        
        // Create two Racecar objects
        Racecar one = new Racecar (ranNum, "E");
        Racecar two = new Racecar (ranNum2, "s");
        
        // Compute the finishing times for both cars
        double time = one.computeTime(distance);
        double time2 = two.computeTime(distance);
        
        // Print times of each car
        System.out.println("First car finished in " + time + " seconds");
        System.out.println("First car finished in " + time2 + " seconds");
    }
}
