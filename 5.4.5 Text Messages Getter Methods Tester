public class Messages
{
    public static void main(String[] args)
    {
        // Your code here.
        // Create two TextMessage objects and print them out.
        TextMessage helloWorld = new TextMessage("Me", "You", "Hello World");
        TextMessage theLorax = new TextMessage("The Lorax", "You", "Save the trees");
        
        System.out.println(helloWorld);
        System.out.println(theLorax);
    }
}
